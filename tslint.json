{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": {
      "options": "ban-single-arg-parens"
    },
    "interface-name": {
      "severity": "off"
    },
    "no-shadowed-variable": {
      "severity": "off"
    },
    "object-literal-sort-keys": {
      "severity": "off"
    },
    "quotemark": {
      "options": "single"
    },
    "variable-name": {
      "options": [
        "allow-leading-underscore",
        "allow-pascal-case",
        "allow-snake-case"
      ]
    }
  },
  "rulesDirectory": []
}
